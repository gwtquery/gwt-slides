<module rename-to='gwtcreate2013'>
    <inherits name='org.gquery.slides.Slides'/>

    <inherits name='com.google.gwt.query.JsQuery'/>
    <inherits name="com.watopi.chosen.Chosen"/>

    <entry-point class='org.gquery.slides.presentations.gwtcreate2013.GwtCreate2013EntryPoint'/>

    <source path="presentations/gwtcreate2013" />
    <public path="presentations/gwtcreate2013/public" />
    
    <add-linker name="xsiframe"/>
    
    <set-property name="user.agent" value="safari, gecko1_8" />    
    <collapse-property name="user.agent" values="*" />
    
    <set-property name="compiler.stackMode" value="strip" />
</module>
