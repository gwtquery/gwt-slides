<module rename-to='slides'>
    <inherits name='com.google.gwt.query.Query'/>

    <entry-point class='org.gquery.slides.client.Slides'/>

    <generate-with class="org.gquery.slides.bind.SlidesGenerator">
        <when-type-assignable class="org.gquery.slides.client.SlidesSourceMarker"/>
    </generate-with>

    <replace-with class="org.gquery.slides.client.Utils.Console">
        <when-type-assignable class="com.google.gwt.query.client.Console"/>
    </replace-with>

    <add-linker name="xsiframe"/>
    
    <set-property name="user.agent" value="safari, gecko1_8" />    
    <collapse-property name="user.agent" values="*" />
    
    <set-property name="compiler.stackMode" value="strip" />
    <set-configuration-property name="compiler.emulatedStack.recordLineNumbers" value="false" />
    <set-configuration-property name="compiler.emulatedStack.recordFileNames" value="false" />
        
</module>

